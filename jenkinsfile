pipeline {
    agent any
    environment {
        test = 'hello'
        test1 = 'world'
    }
    stages {
        stage ('first') {
            steps{
                echo "hello world"    
            }
        }
        stage ('second') {
            steps{
                sh "echo ${test}"
                sh "echo ${test1}"
            }
        }    
        
    }
}