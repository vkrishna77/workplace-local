pipeline {
    agent any
    environment {
        test = 'hello'
        test1 = 'world'
    }
    stages {
        stage ('first') {
            steps{
                git https://github.com/vkrishna77/workplace-local.git
                cat test.html    
            }
        }
        stage ('second') {
            steps{
                sh "echo ${test}"
                sh "echo ${test1}"
            }
        }    
        
    }
}