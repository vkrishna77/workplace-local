pipeline {
    agent any
    environment {
        test = 'hello'
        test1 = 'world'
    }
    stages {
        stage ('first') {
            agent { label "azureagentssh" }
            steps{
                //sh "git fetch https://github.com/vkrishna77/workplace-local.git"
                sh "java -version"
                sh "javac helloworld.java"
                sh "java helloworld"
            }
        }
        stage ('second') {
            agent { label "azureagentssh" }
            steps{
                sh "echo ${test}"
                sh "echo ${test1}"
            }
        }    
        stage ('third') {
            steps{
                //bat "git fetch https://github.com/vkrishna77/workplace-local.git"
                //bat "python test.py"
                sh "echo hello"
            }
        }
        
    }
}