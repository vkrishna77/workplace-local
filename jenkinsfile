pipeline {
    agent any
    environment {
        test = 'hello'
        test1 = 'world'
    }
    stages {
        stage ('first') {
            steps{
                sh "rm -r workplace-local"
                sh "git clone https://github.com/vkrishna77/workplace-local.git"
                sh "curl test.html"    
            }
        }
        stage ('second') {
            steps{
                sh "echo ${test}"
                sh "echo ${test1}"
            }
        }    
        
    }
}